{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp@latest"]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest",
        "--browserUrl=http://localhost:9222",
        "--logFile=/tmp/chrome-devtools-mcp.log",
        "--no-category-emulation",
        "--no-category-performance",
        "--no-category-network"
      ],
      "env": {}
    }
  }
}
